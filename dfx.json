{
  "version": 1,
  "canisters": {
    "zerolock_backend": {
      "type": "rust",
      "package": "zerolock",
      "candid": "src/zerolock_backend.did"
    },
    "zerolock_frontend": {
      "type": "assets",
      "source": ["frontend/dist"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
}